{
    "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],

  "tsserver.formatOnType": false,
  "coc.preferences.formatOnType": false,
  "javascript.validate.enable": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.enableMessage": "jump",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": "â–ˆ ",
  "diagnostic.virtualTextLines": 1,
  "explorer.width": 50,
  "explorer.icon.enableNerdfont": false,
  "explorer.file.showHiddenFiles": true,
  "explorer.keyMappings.global": {
    "<2-LeftMouse>": false,
    "t": false,
    "<bs>": false,
    "F": false,
    "il": false,
    "al": false,
    "ii": false,
    "ai": false,

    "[d": false,
    "]d": false,
    "[D": false,
    "]D": false,

    "[c": false,
    "]c": false,

    "U": ["wait", "gotoParent"],
    "<<": "gitUnstage",
    ">>": "gitStage",
    "cd": "cd",
    "r": "refresh",
    "f": "searchRecursive",
    "R": "refresh",
    "rn": "rename",

    "<cr>": ["expandable?", "expand", "open"],

    "[h": "markPrev:git",
    "]h": "markNext:git",

    "[g": "markPrev:diagnosticError:diagnosticWarning",
    "]g": "markNext:diagnosticError:diagnosticWarning",
    "[G": "markPrev:diagnosticError",
    "]G": "markNext:diagnosticError"

  }
}


